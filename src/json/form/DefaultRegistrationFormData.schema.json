{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default Registration Form Data",
  "type": "object",
  "required": ["email", "password"],
  "properties": {
    "firstName": {
      "type": "string",
      "maxLength": 1024
    },
    "lastName": {
      "type": "string",
      "maxLength": 1024
    },
    "email": {
      "type": "string",
      "format": "email",
      "maxLength": 1024
    },
    "password": {
      "type": "string"
    }
  }
}
