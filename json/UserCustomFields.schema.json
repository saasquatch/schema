{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "User Custom Fields Schema",
	"description": "Defines how custom fields can be specified on a user. Allows alphanumeric numbers and underscores.",
	"type": "object",
	"patternProperties": {
		"^[a-zA-Z0-9_]*$": {
			"oneOf": [
				{
					"type": "string",
					"maxLength": 1024
				},
				{
					"type": "boolean"
				},
				{
					"type": "integer"
				},
				{
					"type": "number"
				},
				{
					"type": "null"
				}
			]
		}
	},
	"additionalProperties": false,
	"maxProperties": 10,
	"id": "https://unpkg.com/@saasquatch/schema@1.0.2-2/json/UserCustomFields.schema.json"
}
