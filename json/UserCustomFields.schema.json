{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "User Custom Fields Schema",
	"description": "Defines how custom fields can be specified on a user. Allows alphanumeric numbers and underscores.",
	"type": "object",
	"patternProperties": {
		"^[a-zA-Z0-9_]{1,64}$": {
			"anyOf": [
				{
					"type": "string",
					"maxLength": 1024
				},
				{
					"type": "boolean"
				},
				{
					"type": "number"
				},
				{
					"type": "null"
				}
			]
		}
	},
	"additionalProperties": false,
	"maxProperties": 100,
	"id": "https://unpkg.com/@saasquatch/schema@1.5.1-0/json/UserCustomFields.schema.json"
}
